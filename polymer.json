{
  "entrypoint": "index.html",
  "shell": "src/dhl-app.html",
  "fragments": [
    "src/dhl-signin.html",
    "src/dhl-trips.html",
    "src/dhl-route.html",
    "src/dhl-delivery.html",
    "src/dhl-home.html",
    "src/dhl-barcodegenerator.html",
    "src/dhl-view404.html"
  ],
  "sources": [
    "src/**/*",
    "images/**/*",
    "bower.json"
